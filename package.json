{
  "name": "specron-example",
  "description": ".",
  "version": "0.0.0",
  "scripts": {
    "build": "echo 'nothing'",
    "compile": "specron compile",
    "sandbox": "specron sandbox",
    "prepare": "specron compile",
    "test": "specron compile && specron test",
    "test-coverage": "./node_modules/.bin/solidity-coverage"
  },
  "specron": {
    "compiler": {
      "build": "./build/contracts",
      "match": [
        "./contracts/**/*.sol"
      ]
    },
    "sandbox": {
      "port": 8545,
      "host": "localhost"
    },
    "test": {
      "server": false,
      "port": 8545,
      "host": "localhost",
      "match": [
        "./tests/**/*.test.*"
      ]
    },
    "require": [
      "ts-node/register"
    ]
  },
  "license": "MIT",
  "dependencies": {
    "@specron/cli": "latest",
    "@specron/spec": "latest",
    "solc": "^0.4.25",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.1",
    "web3": "^1.0.0-beta.36"
  },
  "devDependencies": {
    "solidity-coverage": "^0.6.7"
  }
}
